{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.1.17",
        "andreaselia/laravel-api-to-postman": "^1.10",
        "barryvdh/laravel-debugbar": "^3.5",
        "bugsnag/bugsnag-laravel": "^2.0",
        "cviebrock/eloquent-sluggable": "^10.0",
        "doctrine/dbal": "^3.7",
        "f9webltd/laravel-api-response-helpers": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.10.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "jamesmills/laravel-timezone": "^1.11",
        "laravel/cashier-paddle": "^1.8",
        "laravel/framework": "^10.33.0",
        "laravel/helpers": "^1.4",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "opcodesio/log-viewer": "^3.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^7.2",
        "saloonphp/laravel-plugin": "^3.0",
        "saloonphp/pagination-plugin": "^2.0",
        "saloonphp/saloon": "^3.0",
        "spatie/image-optimizer": "^1.6",
        "spatie/laravel-backup": "^8.4",
        "spatie/laravel-permission": "^6.3",
        "spatie/laravel-searchable": "^1.11.2",
        "torann/geoip": "^3.0",
        "vonage/client": "^3.0"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^1.5",
        "fakerphp/faker": "^1.9.1",
        "glhd/laravel-dumper": "^1.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": ["php artisan storage:link --quiet"]
    }
}
