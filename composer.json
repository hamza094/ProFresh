{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2.17",
    "barryvdh/laravel-debugbar": "^3.5",
    "bugsnag/bugsnag-laravel": "^2.0",
    "cviebrock/eloquent-sluggable": "^11.0",
    "dedoc/scramble": "^0.13.2",
    "doctrine/dbal": "^3.7",
    "f9webltd/laravel-api-response-helpers": "^2.1.1",
    "firebase/php-jwt": "^6.10",
    "friendsofphp/php-cs-fixer": "^3.10.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "intervention/image": "^2.5",
    "laragear/two-factor": "^3.0",
    "laravel/cashier-paddle": "^2.6",
    "laravel/framework": "^11.0",
    "laravel/helpers": "^1.4",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^5.6",
    "laravel/tinker": "^2.9",
    "league/flysystem-aws-s3-v3": "^3.0",
    "maatwebsite/excel": "^3.1",
    "opcodesio/log-viewer": "^3.0",
    "predis/predis": "^3.2",
    "pusher/pusher-php-server": "^7.2",
    "saloonphp/laravel-plugin": "^3.0",
    "saloonphp/pagination-plugin": "^2.0",
    "saloonphp/rate-limit-plugin": "^2.0",
    "saloonphp/saloon": "^3.0",
    "spatie/image-optimizer": "^1.6",
    "spatie/laravel-backup": "^8.4",
    "spatie/laravel-permission": "^6.3",
    "torann/geoip": "^3.0",
    "vonage/client": "^3.0",
    "wendelladriel/laravel-validated-dto": "^3.5",
    "jamesmills/laravel-timezone": "dev-master as 1.7.0"
  },
  "require-dev": {
    "enlightn/enlightn": "^2.10",
    "fakerphp/faker": "^1.23",
    "kitloong/laravel-migrations-generator": "^7.2",
    "larastan/larastan": "^2.0",
    "laravel/pint": "^1.25",
    "laravel/telescope": "^5.2",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.1",
    "phpunit/phpunit": "^11.0.1",
    "rector/rector": "^1.2",
    "spatie/laravel-ignition": "^2.0",
    "spaze/phpstan-disallowed-calls": "^4.7",
    "squizlabs/php_codesniffer": "3.*",
    "thecodingmachine/phpstan-safe-rule": "^1.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": []
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-install-cmd": [
      "php artisan storage:link --quiet"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "test": [
      "@php vendor/bin/phpunit"
    ],
    "stan": [
      "@php vendor/bin/phpstan analyse -c phpstan.neon --memory-limit=1G"
    ],
    "pint:test": [
      "@php vendor/bin/pint --test"
    ],
    "rector:test": [
      "@php vendor/bin/rector --dry-run"
    ],
    "qa": [
      "@test",
      "@stan",
      "@pint:test",
      "@rector:test"
    ],
    "qa:fix": [
      "@php vendor/bin/rector",
      "@php vendor/bin/pint"
    ],
    "qa:all": [
      "@php tools/qa-all.php"
    ]
  }
}
